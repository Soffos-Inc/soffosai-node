{
  "name": "soffosai",
  "version": "0.1.2",
  "description": "Javascript package for Soffos.ai API",
  "main": "./dist/app.js",
  "module": "./src/app.js",
  "jsnext:main": "./src/app.js",
  "typings": "./index.d.ts",
  "types": "index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/app.js",
      "require": "./dist/app.js"
    },
    "./nodes": "./src/core/nodes/index.js",
    "./pipelines": "./src/core/pipelines/index.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node src/app.js",
    "transpile": "babel src --out-dir dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Soffos-Inc/soffosai-js.git"
  },
  "author": "franco such",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Soffos-Inc/soffosai-js/issues"
  },
  "homepage": "https://github.com/Soffos-Inc/soffosai-js#readme",
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9"
  },
  "keywords": [
    "soffos",
    "soffos.ai",
    "AI"
  ]
}
